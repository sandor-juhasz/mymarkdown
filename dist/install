#!/bin/bash -e

echo "Creating destination directories..."
mkdir -p ~/.local/share/mymarkdown
mkdir -p ~/.local/bin

echo "Copying mymarkdown.jar..."
cp mymarkdown.jar ~/.local/share/mymarkdown/mymarkdown.jar

echo "Copying mymarkdown wrappers..."
cp mymarkdown ~/.local/bin
cp process-notes-dir ~/.local/bin

echo "Done"
